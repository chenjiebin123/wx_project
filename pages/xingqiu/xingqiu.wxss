Page {
    height: 100%;
    width: 100%;
}
.xingqiu {
    width: 100%;
    height: 100%;
    background-color: #eeeeee;
    padding: 40rpx;
    box-sizing: border-box;
}
.xingqiu .list {
    width: 100%;
    display: flex;
    flex-wrap: wrap;
}
.xingqiu .detail {
    height: 120rpx;
    width: 210rpx;
    border-radius: 12rpx;
    background-color: #fff;
    position: relative;
    margin-bottom: 10rpx;
    margin-left: 10rpx;
}
.xingqiu .list .icon {
    width: 28rpx;
    height: 9rpx;
    background-color: red;
    border-radius: 20rpx;
    position:absolute;
    top: 15rpx;
    left: 20rpx;
}
.xingqiu .list navigator:nth-child(1) .detail .icon {
    background-color:rgb(255, 0, 0);
}
.xingqiu .list navigator:nth-child(2) .detail .icon {
    background-color:rgb(235, 65, 13);
}
.xingqiu .list navigator:nth-child(3) .detail .icon {
    background-color:rgb(255, 145, 0);
}
.xingqiu .list navigator:nth-child(4) .detail .icon {
    background-color:rgb(255, 196, 0);
}
.xingqiu .list navigator:nth-child(5) .detail .icon {
    background-color:rgb(187, 255, 0);
}
.xingqiu .list navigator:nth-child(6) .detail .icon {
    background-color:rgb(0, 255, 34);
}
.xingqiu .list navigator:nth-child(7) .detail .icon {
    background-color:rgb(0, 255, 157);
}
.xingqiu .list navigator:nth-child(8) .detail .icon {
    background-color:rgb(9, 172, 118);
}
.xingqiu .list navigator:nth-child(9) .detail .icon {
    background-color:rgb(0, 225, 255);
}

.xingqiu .list .text {
    position: absolute;
    top: 50%;
    margin-top: -20rpx;
    width: 100%;
    text-align: center;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 26rpx;
}